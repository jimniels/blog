{
  "name": "jimniels-blog",
  "version": "1.0.0",
  "description": "[jim-nielsen.com/blog](http://jim-nielsen.com/blog)",
  "main": "metalsmith.js",
  "dependencies": {
    "highlight.js": "^9.15.8",
    "marked": "^0.7.0",
    "metalsmith": "^2.3.0",
    "metalsmith-serve": "^0.0.7",
    "metalsmith-watch": "^1.0.3",
    "multimatch": "^3.0.0",
    "prop-types": "^15.7.2",
    "shx": "^0.3.2"
  },
  "devDependencies": {},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "date": "node date.js",
    "copy-css": "cp node_modules/highlight.js/styles/atom-one-*.css src/client/assets/css/",
    "pre": "npm run copy-css && shx mkdir -p src/client/posts/ && shx cp src/content/*.md src/client/posts/ && shx cp -R src/content/images src/client/images",
    "prestart": "npm run pre",
    "start": "node metalsmith.js",
    "prebuild": "npm run pre",
    "build": "NODE_ENV=production node metalsmith.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimniels/blog.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jimniels/blog/issues"
  },
  "homepage": "https://github.com/jimniels/blog#readme"
}
